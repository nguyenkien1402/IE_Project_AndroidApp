<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeSliderRangePicker">
        <!--<attr name="start_angle" format="float" />-->
        <!--<attr name="end_angle" format="float" />-->
        <attr name="border_thickness" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="thumb_size" format="dimension" />
        <attr name="start_thumb_size" format="dimension" />
        <attr name="end_thumb_size" format="dimension" />
        <attr name="start_thumb_image_am" format="reference" />
        <attr name="end_thumb_image_am" format="reference" />
        <attr name="start_thumb_image_pm" format="reference" />
        <attr name="end_thumb_image_pm" format="reference" />
        <attr name="start_thumb_color" format="color" />
